{
   "files.watcherExclude": {
      "**/build/**": true,
      "**/dist/**": true,
      "**/www/**": true
   },
   "editor.tabSize": 3,
   "[javascript]": {
      "editor.tabSize": 3
   },
   "[json]": {
      "editor.tabSize": 3
   },
   "workbench.tree.indent": 15,
   "files.trimTrailingWhitespace": true,
   "window.zoomLevel": -1,
   "files.autoSave": "onWindowChange",
   "diffEditor.ignoreTrimWhitespace": false,
   "workbench.iconTheme": "material-icon-theme"
}
